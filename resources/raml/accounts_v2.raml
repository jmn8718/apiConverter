/api/v2/accounts:
  description: ""
  get:
    description: |
      Method to return accounts basic information for the authenticated user

      * Account types

      code | description |
      --- | ---
      01 | checkings |
      02 | savings |
    headers:
      Accept:
        default: application/json
        required: true
      Accept-Language:
        default: en, en-US
        required: true
      Content-Type:
        default: application/json
        required: true
      Authorization:
        default: "Tsec <token>"
        required: true
      X-Unique-Transaction-ID:
        default: f99e5ae0-2e9f-11e4-8c21-0800200c9a66
        required: true
    queryParameters:
      type:
        type: string
        description: |
          Return only accounts of the specified type.

